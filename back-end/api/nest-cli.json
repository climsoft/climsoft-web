{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "sql-scripts/**/*.sql",
        "watchAssets": true
      },
      {
        "include": "observation/services/export/**/*.json",
        "watchAssets": true
      }
    ]
  }
}
